@article{Chin2008,
  abstract = {The problem of discovering novel motifs of binding sites is important to the understanding of gene regulatory networks. Motifs are generally represented by matrices (position weight matrix (PWM) or position specific scoring matrix (PSSM) or strings. However, these representations cannot model biological binding sites well because they fail to capture nucleotide interdependence. It has been pointed out by many researchers that the nucleotides of the DNA binding site cannot be treated independently, e.g. the binding sites of zinc finger in proteins. In this paper, a new representation called Scored Position Specific Pattern (SPSP), which is a generalization of the matrix and string representations, is introduced which takes into consideration the dependent occurrences of neighboring nucleotides. Even though the problem of discovering the optimal motif in SPSP representation is proved to be NP-hard, we introduce a heuristic algorithm called SPSP-Finder, which can effectively find optimal motifs in most simulated cases and some real cases for which existing popular motif finding software, such as Weeder, MEME and AlignACE, fail.},
  author = {Chin, F and Leung, H C},
  doi = {10.1109/TCBB.2007.70220},
  issn = {1545-5963},
  journal = {IEEE/ACM transactions on computational biology and bioinformatics / IEEE, ACM},
  keywords = {biology, string,algorithms},
  number = {1},
  pages = {110--119},
  title = {{\{DNA\} motif representation with nucleotide dependency.}},
  url = {http://dx.doi.org/10.1109/TCBB.2007.70220},
  volume = {5},
  year = {2008},
}

